{
  "name": "collect.js",
  "version": "3.0.40",
  "description": "Convenient and dependency free wrapper for working with arrays and objects.",
  "main": "dist/collect.js",
  "typings": "index.d.ts",
  "scripts": {
    "pretest": "npm run build",
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "cross-env NODE_ENV=development webpack --watch",
    "test": "mocha test/tests.js && karma start",
    "reporter": "nyc report --reporter=html",
    "coverage": "nyc mocha test/tests.js",
    "test:watch": "mocha -w test/tests.js",
    "eslint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ecrmnn/collect.js"
  },
  "keywords": [
    "collections",
    "collection",
    "collect",
    "map",
    "filter",
    "group by",
    "reduce",
    "transform",
    "pluck",
    "pie",
    "mapWithKeys",
    "laravel",
    "whereIn",
    "values",
    "unique"
  ],
  "author": {
    "name": "Daniel Eckermann",
    "email": "danieleckermann@gmail.com",
    "url": "http://danieleckermann.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecrmnn/collect.js/issues"
  },
  "homepage": "https://github.com/ecrmnn/collect.js",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "benchmark": "^2.1.0",
    "chai": "^3.5.0",
    "cross-env": "^5.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-import": "^2.5.0",
    "karma": "^1.7.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-mocha": "^1.3.0",
    "mocha": "^2.4.5",
    "nyc": "^11.0.2",
    "webpack": "^3.0.0"
  }
}